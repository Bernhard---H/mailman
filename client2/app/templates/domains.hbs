<div class="row">
  <div class="col-12 col-sm-10 col-md-8 col-lg-7 col-xl-6 my-5 mx-auto">
    <PowerSelect
      @placeholder="select your domain ..."
      @selected={{this.selected}}
      @options={{this.model}}
      @onChange={{fn this.onSelectionChange}}
      as |domain|>
      {{domain.domain}}
    </PowerSelect>
  </div>
</div>

<div class="row">
  {{outlet}}
</div>